DIM asteroid1%(50)
DIM asteroid2%(50)
DIM asteroid3%(50)
DIM asteroid4%(50)
DIM cometleft1%(100) 'sprites for the comets which spawn on the left side and head right
DIM cometleft2%(100)
DIM cometright1%(100) 'sprites for the comets that spawn on the right and head left
DIM cometright2%(100)

RANDOMIZE TIMER

DIM asteroidspawnside,cometspawnside, asteroidspeed, cometspeed, spawntimer, asteroidtype, maxasteroids, maxcomets,cometspawntimer AS INTEGER

DIM SHARED asteroidactive(5),asteroidx(5),asteroidy(5),asteroiddirection(5),asteroidanimation(5) 'values for asteroids

DIM SHARED cometactive(2),cometx(2),comety(2),cometdirection(2),cometanimation(2) 'values for comets. They only have 2 animation states instead of 4 btw

asteroidspawntimer = 0
maxasteroids = 5
asteroidspeed = 4
cometspawntimer = 0
cometspeed = 8
maxcomets = 2
                                                                                                                        
SCREEN 7                                                                                                                 
                                                                                                                         
FOR Y = 0 TO 59  ' For each row                                                                                                                                                                                                   
  FOR X = 0 TO 103  ' For each column
    READ z
    PSET (X, Y), z 'prints all initial sprites then we need to capture them for re-use
  NEXT X
NEXT Y

GET (1,1)-(7,6), asteroid1% 'horizontally facing
GET (9,1)-(14,7), asteroid2% 'vertically
GET (1,8)-(7,13), asteroid3% 'horizontally
GET (9,9)-(14,15), asteroid4% 'vertically
GET (22,0)-(37,11), cometleft1% 'spawns on the left, goes right, small tail
GET (18,13)-(37,24), cometleft2% 'spawns on the left, goes right, big tail
GET (40,0)-(59,11), cometright1% 'spawns on the right, goes left, small tail
GET (40,13)-(55,24), cometright2% 'spawns on the right, goes left, big tail

SLEEP

CLS

DO

	asteroidspawntimer = asteroidspawntimer + 5 'the timer for when an asteroid spawns. max can be set below
	cometspawntimer = cometspawntimer + 5 'same as the asteroid spawn timer, can be set in its block below

	IF asteroidspawntimer = 100 THEN 'set the timer here. bigger number = longer time between spawns. 
		asteroidspawntimer = 0
		asteroidspawnside = INT(RND * 2) + 1 'randomizes what side the asteroid spawn on. Either 1 or 2. 1 is left, 2 is right
		FOR i = 1 TO maxasteroids
			IF asteroidactive(i) = 0 THEN
				asteroidactive(i) = 1
					IF asteroidspawnside = 1 THEN 'from the left
							asteroiddirection(i) = 1
							asteroidanimation(i) = 1
							asteroidx(i) = 5
							asteroidy(i) = 5
							PUT (5,5), asteroid1%
						ELSEIF asteroidspawnside = 2 THEN ' from the right
							asteroiddirection(i) = 2
							asteroidanimation(i) = 4
							asteroidx(i) = 310
							asteroidy(i) = 5
							PUT (310,5), asteroid4%
					ENDIF
				EXIT FOR
			ENDIF
		NEXT i
	ENDIF
	
	IF cometspawntimer = 500 THEN 'set the timer here. bigger number = longer time between spawns. 
		cometspawntimer = 0
		cometspawnside = INT(RND * 2) + 1 'randomizes what side the comet spawn on. Either 1 or 2. 1 is left, 2 is right
		FOR i = 1 TO maxcomets
			IF cometactive(i) = 0 THEN
				cometactive(i) = 1
					IF cometspawnside = 1 THEN 'from the left
							cometdirection(i) = 1
							cometanimation(i) = 1
							cometx(i) = 5
							comety(i) = 15
							PUT (5,15), cometleft1%
						ELSEIF cometspawnside = 2 THEN ' from the right
							cometdirection(i) = 2
							cometanimation(i) = 3
							cometx(i) = 299
							comety(i) = 15
							PUT (299,15), cometright1%
					ENDIF
				EXIT FOR
			ENDIF
		NEXT i
	ENDIF
		
	FOR i = 1 TO maxasteroids
		IF asteroidactive(i) = 1 AND asteroiddirection(i) = 1 THEN
				asteroidx(i) = asteroidx(i) + asteroidspeed
				IF asteroidx(i) > 310 THEN
						asteroiddirection(i) = 2
						asteroidy(i) = asteroidy(i) + 10
						IF asteroidanimation(i) = 4 THEN 'deletes the asteroids when they "go off screen"
								PUT (asteroidx(i) - asteroidspeed, asteroidy(i) - 10), asteroid4%
								asteroidx(i) = 310
								asteroidanimation(i) = 4
								PUT (asteroidx(i), asteroidy(i)), asteroid4%
							ELSEIF asteroidanimation(i) = 3 THEN
								PUT (asteroidx(i) - asteroidspeed, asteroidy(i) - 10), asteroid3%
								asteroidx(i) = 310
								asteroidanimation(i) = 4
								PUT (asteroidx(i), asteroidy(i)), asteroid4%
							ELSEIF asteroidanimation(i) = 2 THEN
								PUT (asteroidx(i) - asteroidspeed, asteroidy(i) - 10), asteroid2%
								asteroidx(i) = 310
								asteroidanimation(i) = 4
								PUT (asteroidx(i), asteroidy(i)), asteroid4%
							ELSEIF asteroidanimation(i) = 1 THEN
								PUT (asteroidx(i) - asteroidspeed, asteroidy(i) - 10), asteroid1%
								asteroidx(i) = 310
								asteroidanimation(i) = 4
								PUT (asteroidx(i), asteroidy(i)), asteroid4%
						ENDIF
					ELSEIF asteroidanimation(i) = 1 AND asteroidactive(i) = 1 THEN
						PUT (asteroidx(i) - asteroidspeed,asteroidy(i)), asteroid1% 'deletes the old asteroid image. remember, it was at the old area, so we minus the asteroid speed
						asteroidanimation(i) = 2
						PUT (asteroidx(i),asteroidy(i)), asteroid2%
					ELSEIF asteroidanimation(i) = 2 AND asteroidactive(i) = 1 THEN
						PUT (asteroidx(i) - asteroidspeed,asteroidy(i)), asteroid2%
						asteroidanimation(i) = 3
						PUT (asteroidx(i),asteroidy(i)), asteroid3%
					ELSEIF asteroidanimation(i) = 3 AND asteroidactive(i) = 1 THEN
						PUT (asteroidx(i) - asteroidspeed,asteroidy(i)), asteroid3%
						asteroidanimation(i) = 4
						PUT (asteroidx(i),asteroidy(i)), asteroid4%
					ELSEIF asteroidanimation(i) = 4 AND asteroidactive(i) = 1 THEN
						PUT (asteroidx(i) - asteroidspeed,asteroidy(i)), asteroid4%
						asteroidanimation(i) = 1
						PUT (asteroidx(i),asteroidy(i)), asteroid1%
				ENDIF
			ELSEIF asteroidactive(i) = 1 AND asteroiddirection(i) = 2 THEN
				asteroidx(i) = asteroidx(i) - asteroidspeed
				IF asteroidx(i) < 10 THEN
						asteroiddirection(i) = 1
						asteroidy(i) = asteroidy(i) + 10
						IF asteroidanimation(i) = 4 THEN
								PUT (asteroidx(i) + asteroidspeed, asteroidy(i) - 10), asteroid4%
								asteroidx(i) = 5
								asteroidanimation(i) = 1
								PUT (asteroidx(i), asteroidy(i)), asteroid1%
							ELSEIF asteroidanimation(i) = 3 THEN
								PUT (asteroidx(i) + asteroidspeed, asteroidy(i) - 10), asteroid3%
								asteroidx(i) = 5
								asteroidanimation(i) = 1
								PUT (asteroidx(i), asteroidy(i)), asteroid1%
							ELSEIF asteroidanimation(i) = 2 THEN
								PUT (asteroidx(i) + asteroidspeed, asteroidy(i) - 10), asteroid2%
								asteroidx(i) = 5
								asteroidanimation(i) = 1
								PUT (asteroidx(i), asteroidy(i)), asteroid1%
							ELSEIF asteroidanimation(i) = 1 THEN
								PUT (asteroidx(i) + asteroidspeed, asteroidy(i) - 10), asteroid1%
								asteroidx(i) = 5
								asteroidanimation(i) = 1
								PUT (asteroidx(i), asteroidy(i)), asteroid1%
						ENDIF
					ELSEIF asteroidanimation(i) = 4 AND asteroidactive(i) = 1 THEN
						PUT (asteroidx(i) + asteroidspeed,asteroidy(i)), asteroid4%
						asteroidanimation(i) = 3
						PUT (asteroidx(i),asteroidy(i)), asteroid3%
					ELSEIF asteroidanimation(i) = 3 AND asteroidactive(i) = 1 THEN
						PUT (asteroidx(i) + asteroidspeed,asteroidy(i)), asteroid3%
						asteroidanimation(i) = 2
						PUT (asteroidx(i),asteroidy(i)), asteroid2%
					ELSEIF asteroidanimation(i) = 2 AND asteroidactive(i) = 1 THEN
						PUT (asteroidx(i) + asteroidspeed,asteroidy(i)), asteroid2%
						asteroidanimation(i) = 1
						PUT (asteroidx(i),asteroidy(i)), asteroid1%
					ELSEIF asteroidanimation(i) = 1 AND asteroidactive(i) = 1 THEN
						PUT (asteroidx(i) + asteroidspeed,asteroidy(i)), asteroid1%
						asteroidanimation(i) = 4
						PUT (asteroidx(i),asteroidy(i)), asteroid4%
				ENDIF
		ENDIF
	NEXT i
	
	
	
	FOR i = 1 TO maxcomets
		IF cometactive(i) = 1 AND cometdirection(i) = 1 THEN
				cometx(i) = cometx(i) + cometspeed
				IF cometx(i) > 300 THEN
						cometdirection(i) = 2
						comety(i) = comety(i) + 30
						IF cometanimation(i) = 1 THEN
								PUT (cometx(i) - cometspeed,comety(i) - 30), cometleft1%
								cometx(i) = 299
								cometanimation(i) = 3
								PUT (cometx(i),comety(i)),cometright1%
							ELSEIF cometanimation(i) = 2 THEN
								PUT (cometx(i) - cometspeed,comety(i) - 30), cometleft2%
								cometx(i) = 299
								cometanimation(i) = 3
								PUT (cometx(i),comety(i)),cometright1%
						ENDIF				
					ELSEIF cometanimation(i) = 1 THEN
						PUT (cometx(i) - cometspeed,comety(i)), cometleft1%
						cometanimation(i) = 2
						PUT (cometx(i),comety(i)), cometleft2%
					ELSEIF cometanimation(i) = 2 THEN
						PUT (cometx(i) - cometspeed,comety(i)), cometleft2%
						cometanimation(i) = 1
						PUT (cometx(i),comety(i)), cometleft1%
				ENDIF
			ELSEIF cometactive(i) = 1 AND cometdirection(i) = 2 THEN
				cometx(i) = cometx(i) - cometspeed
				IF cometx(i) < 9 THEN
						cometdirection(i) = 1
						comety(i) = comety(i) + 30  
						IF cometanimation(i) = 3 THEN
								PUT (cometx(i) + cometspeed,comety(i) - 30), cometright1%
								cometx(i) = 5
								cometanimation(i) = 1
								PUT (cometx(i),comety(i)), cometleft1%
							ELSEIF cometanimation(i) = 4 THEN
								PUT (cometx(i) + cometspeed,comety(i) - 30), cometright2%
								cometx(i) = 5
								cometanimation(i) = 1
								PUT (cometx(i),comety(i)), cometleft1%
						ENDIF				
					ELSEIF cometanimation(i) = 3 THEN
						PUT (cometx(i) + cometspeed,comety(i)), cometright1%
						cometanimation(i) = 4
						PUT (cometx(i),comety(i)), cometright2%
					ELSEIF cometanimation(i) = 4 THEN
						PUT (cometx(i) + cometspeed,comety(i)), cometright2%
						cometanimation(i) = 3
						PUT (cometx(i),comety(i)), cometright1%
				ENDIF
		ENDIF
	NEXT i

	uin$ = INKEY$ 'gotta be able to close the program lol
			SELECT CASE uin$
				CASE CHR$(0) + CHR$(83): GOTO 1 'goes to 1 and ends the program. delete key btw
			END SELECT
	

	FOR I = 1 to 50: NEXT I 'timer for gameplay loop. Smaller number = faster game

LOOP

1

