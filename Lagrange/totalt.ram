						ENDIF
					ELSEIF bigasteroiddropped = 1 THEN
						PUT (bigasteroidx(i),bigasteroidy(i)), bigasteroid4%
						bigasteroiddropped(i) = 0
					ELSEIF bigasteroidanimation(i) = 4 AND bigasteroidactive(i) = 1 AND bigasteroiddamaged(i) = 0 THEN
						PUT (bigasteroidx(i) + bigasteroidspeed,bigasteroidy(i)), bigasteroid4% 'deletes the old asteroid image. remember, it was at the old area, so we minus the asteroid speed
						bigasteroidanimation(i) = 3
						PUT (bigasteroidx(i),bigasteroidy(i)), bigasteroid3%
					ELSEIF bigasteroidanimation(i) = 3 AND bigasteroidactive(i) = 1 AND bigasteroiddamaged(i) = 0 THEN
