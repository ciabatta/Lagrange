DATA 00,00,04,04,00,00,00,04,04,04,00,00,00,04,04,00,00,00,00,00,00,00,00,00,00,00,00,00,00,00,00,00,00,00,00,00,00,00,00,00,00,00,00,00,00,00,00,00,00,00,00,00,00,00,00,00,00,00,00,00,00,00,00,00,00,00,00,00,00,00,00,00,00,00,00,00,00,00,00,00,00,00,00,00,00,00,00,00,00,00,00,00,00,00,00,00,00,00,00,00,00,00,00,00

'log: next we need to implement the big asteroids and the comets collision with the projectiles.

DIM asteroid1%(50)
DIM asteroid2%(50)
DIM asteroid3%(50)
DIM asteroid4%(50)
DIM cometleft1%(100) 'sprites for the comets which spawn on the left side and head right
DIM cometleft2%(100)
DIM cometright1%(100) 'sprites for the comets that spawn on the right and head left
DIM cometright2%(100)
DIM bigasteroid1%(200)
DIM bigasteroid2%(200)
DIM bigasteroid3%(200)
DIM bigasteroid4%(200)
DIM bigasteroiddamaged1%(200)
DIM bigasteroiddamaged2%(200)
DIM bigasteroiddamaged3%(200)
DIM bigasteroiddamaged4%(200)
DIM spacejunkexplosion1%(100)
DIM spacejunkexplosion2%(150)
DIM centralturret%(100)
DIM centralturretfire%(100)
DIM playerprojectile1%(10)
DIM playerprojectile2%(10)
DIM leftturret%(100)
DIM leftturretfire%(100)
DIM rightturret%(100)
DIM rightturretfire%(100)

RANDOMIZE TIMER

DIM asteroidspawnside, cometspawnside, asteroidspeed, cometspeed, bigasteroidspawntimer, asteroidtype, maxasteroids, maxcomets, cometspawntimer, maxbigasteroids, bigasteroidspeed, bigasteroidspawnside AS INTEGER

DIM playerprojectilespeed, playerprojectilenumber, playerprojectilediagspeed AS INTEGER 'player projectile speeds and max number that can be onscreen at any one time

DIM leftturretx, leftturrety, leftturretanimation, centralturretx, centralturrety, centralturretanimation, rightturretx, rightturrety, rightturretanimation AS INTEGER 'locations for the turrets

DIM SHARED asteroidactive(3),asteroidx(3),asteroidy(3),asteroiddirection(3),asteroidanimation(3) 'values for asteroids

DIM SHARED cometactive(2),cometx(2),comety(2),cometdirection(2),cometanimation(2) 'values for comets. They only have 2 animation states instead of 4 btw

DIM SHARED bigasteroidactive(3),bigasteroidx(3),bigasteroidy(3),bigasteroiddirection(3),bigasteroidanimation(3),bigasteroiddamaged(3),bigasteroiddropped(3) 'the last value is for seeing if the asteroid has been damaged or not for a new set of animations. ugh

DIM SHARED playerprojectileactive(9), playerprojectilex(9), playerprojectiley(9), playerprojectiledirection(9), playerprojectileanimation(9) 'the arrays that hold the data for the player's projectiles

DIM maxexplosions AS INTEGER 'the number of explosions on screen. gotta make it a big number, like 20 or something

DIM SHARED explosionx(10), explosiony(10), explosionactive(10), explosionanimation(10) 'the data for the explosions 

playerprojectilenumber = 9
playerprojectilespeed = 8
playerprojectilediagspeed = 6

leftturretx = 10
leftturrety = 175
leftturretanimation = 0
centralturretx = 145
centralturrety = 175
centralturretanimation = 0
rightturretx = 300
rightturrety = 175
rightturretanimation = 0

asteroidspawntimer = 0
maxasteroids = 3
asteroidspeed = 4
cometspawntimer = 0
cometspeed = 8
maxcomets = 2
maxbigasteroids = 3
bigasteroidspeed = 5
bigasteroidspawntimer = 0

maxexplosions = 10

CLS

SCREEN 7 '320x by 200y pixels

FOR Y = 0 TO 117  ' For each row                                                                                                                                                                                                   
  FOR X = 0 TO 103  ' For each column
    READ z
    PSET (X, Y), z 'prints all initial sprites then we need to capture them for re-use
  NEXT X
NEXT Y

GET (1,1)-(7,6), asteroid1% 'horizontally facing
GET (9,1)-(14,7), asteroid2% 'vertically
GET (1,8)-(7,13), asteroid3% 'horizontally
GET (9,9)-(14,15), asteroid4% 'vertically
GET (22,0)-(37,11), cometleft1% 'spawns on the left, goes right, small tail
GET (18,13)-(37,24), cometleft2% 'spawns on the left, goes right, big tail
GET (40,0)-(59,11), cometright1% 'spawns on the right, goes left, small tail
GET (40,13)-(55,24), cometright2% 'spawns on the right, goes left, big tail
GET (0,60)-(20,87), bigasteroiddamaged1%
GET (42,60)-(62,87), bigasteroiddamaged2% 'two sets of big asteroid sprites, one for the damaged ones, and one for undamaged
GET (21,60)-(41,87), bigasteroiddamaged3%
GET (63,60)-(83,87), bigasteroiddamaged4%
GET (61,0)-(81,27 ), bigasteroid1%
GET (22,30)-(42,57), bigasteroid2%
GET (0,30)-(19,57), bigasteroid3%
GET (83,0)-(103,27), bigasteroid4%
GET (65,33)-(75,45), spacejunkexplosion1% 'this explosion will be used for all "space junk" like asteroids, comets, debirs, etc
GET (46,32)-(62,50), spacejunkexplosion2%

GET (0,90)-(15,101), centralturret% 'capturin the sprites
GET (0,104)-(15,117), centralturretfire%
GET (17,90)-(19,92), playerprojectile1%
GET (17,94)-(19,96), playerprojectile2%
GET (21,90)-(33,101), leftturret%
GET (21,104)-(34,117), leftturretfire%
GET (35,90)-(47,101), rightturret%
GET (35,104)-(48,117), rightturretfire%

SLEEP

CLS

PUT (leftturretx,leftturrety), leftturret% 'puts the turrets at their positions. modify positions up above
PUT (centralturretx,centralturrety), centralturret%
PUT (rightturretx,rightturrety), rightturret% 

DO

	IF leftturretanimation = 1 THEN 'the three IFs below swap back to the turret idle animation, checks every frame
		PUT (leftturretx,leftturrety - 1), leftturretfire% 'position offset for the firing animation
		PUT (leftturretx,leftturrety), leftturret%
		leftturretanimation = 0
	ENDIF
	
	IF centralturretanimation = 1 THEN
		PUT (centralturretx,centralturrety - 2), centralturretfire%
		PUT (centralturretx,centralturrety), centralturret%
		centralturretanimation = 0
	ENDIF
	
	IF rightturretanimation = 1 THEN
		PUT (rightturretx - 1,rightturrety - 1), rightturretfire%
		PUT (rightturretx,rightturrety), rightturret%
		rightturretanimation = 0
	ENDIF
	
	asteroidspawntimer = asteroidspawntimer + 5 'the timer for when an asteroid spawns. max can be set below
	
	IF asteroidspawntimer = 100 THEN 'set the timer here. bigger number = longer time between spawns. 
		asteroidspawntimer = 0
		asteroidspawnside = INT(RND * 2) + 1 'randomizes what side the asteroid spawn on. Either 1 or 2. 1 is left, 2 is right
		FOR i = 1 TO maxasteroids
			IF asteroidactive(i) = 0 THEN
				asteroidactive(i) = 1
					IF asteroidspawnside = 1 THEN 'from the left
							asteroiddirection(i) = 1
							asteroidanimation(i) = 1
							asteroidx(i) = 5
							asteroidy(i) = 5
							PUT (5,5), asteroid1%
						ELSEIF asteroidspawnside = 2 THEN ' from the right
							asteroiddirection(i) = 2
							asteroidanimation(i) = 4
							asteroidx(i) = 310
							asteroidy(i) = 5
							PUT (310,5), asteroid4%
					ENDIF
				EXIT FOR
			ENDIF
		NEXT i
	ENDIF
	
	FOR i = 1 TO maxasteroids
		IF asteroidactive(i) = 1 AND asteroiddirection(i) = 1 THEN
				asteroidx(i) = asteroidx(i) + asteroidspeed
				IF asteroidx(i) > 310 THEN
						asteroiddirection(i) = 2
						asteroidy(i) = asteroidy(i) + 10
						IF asteroidy(i) > 170 THEN
								asteroidactive(i) = 0
							ELSEIF asteroidanimation(i) = 4 AND asteroidactive(i) = 1 THEN 'deletes the asteroids when they "go off screen"
								PUT (asteroidx(i) - asteroidspeed, asteroidy(i) - 10), asteroid4%
								asteroidx(i) = 310
								asteroidanimation(i) = 4
								PUT (asteroidx(i), asteroidy(i)), asteroid4%
							ELSEIF asteroidanimation(i) = 3 AND asteroidactive(i) = 1 THEN
								PUT (asteroidx(i) - asteroidspeed, asteroidy(i) - 10), asteroid3%
								asteroidx(i) = 310
								asteroidanimation(i) = 4
								PUT (asteroidx(i), asteroidy(i)), asteroid4%
							ELSEIF asteroidanimation(i) = 2 AND asteroidactive(i) = 1 THEN
								PUT (asteroidx(i) - asteroidspeed, asteroidy(i) - 10), asteroid2%
								asteroidx(i) = 310
								asteroidanimation(i) = 4
								PUT (asteroidx(i), asteroidy(i)), asteroid4%
							ELSEIF asteroidanimation(i) = 1 AND asteroidactive(i) = 1 THEN
								PUT (asteroidx(i) - asteroidspeed, asteroidy(i) - 10), asteroid1%
								asteroidx(i) = 310
								asteroidanimation(i) = 4
								PUT (asteroidx(i), asteroidy(i)), asteroid4%
						ENDIF
					ELSEIF asteroidanimation(i) = 1 AND asteroidactive(i) = 1 THEN
						PUT (asteroidx(i) - asteroidspeed,asteroidy(i)), asteroid1% 'deletes the old asteroid image. remember, it was at the old area, so we minus the asteroid speed
						asteroidanimation(i) = 2
						PUT (asteroidx(i),asteroidy(i)), asteroid2%
					ELSEIF asteroidanimation(i) = 2 AND asteroidactive(i) = 1 THEN
						PUT (asteroidx(i) - asteroidspeed,asteroidy(i)), asteroid2%
						asteroidanimation(i) = 3
						PUT (asteroidx(i),asteroidy(i)), asteroid3%
					ELSEIF asteroidanimation(i) = 3 AND asteroidactive(i) = 1 THEN
						PUT (asteroidx(i) - asteroidspeed,asteroidy(i)), asteroid3%
						asteroidanimation(i) = 4
						PUT (asteroidx(i),asteroidy(i)), asteroid4%
					ELSEIF asteroidanimation(i) = 4 AND asteroidactive(i) = 1 THEN
						PUT (asteroidx(i) - asteroidspeed,asteroidy(i)), asteroid4%
						asteroidanimation(i) = 1
						PUT (asteroidx(i),asteroidy(i)), asteroid1%
				ENDIF
			ELSEIF asteroidactive(i) = 1 AND asteroiddirection(i) = 2 THEN
				asteroidx(i) = asteroidx(i) - asteroidspeed
				IF asteroidx(i) < 10 THEN
						asteroiddirection(i) = 1
						asteroidy(i) = asteroidy(i) + 10
						IF asteroidy(i) > 170 THEN
								asteroidactive(i) = 0
							ELSEIF asteroidanimation(i) = 4 AND asteroidactive(i) = 1 THEN
								PUT (asteroidx(i) + asteroidspeed, asteroidy(i) - 10), asteroid4%
								asteroidx(i) = 5
								asteroidanimation(i) = 1
								PUT (asteroidx(i), asteroidy(i)), asteroid1%
							ELSEIF asteroidanimation(i) = 3 AND asteroidactive(i) = 1 THEN
								PUT (asteroidx(i) + asteroidspeed, asteroidy(i) - 10), asteroid3%
								asteroidx(i) = 5
								asteroidanimation(i) = 1
								PUT (asteroidx(i), asteroidy(i)), asteroid1%
							ELSEIF asteroidanimation(i) = 2 AND asteroidactive(i) = 1 THEN
								PUT (asteroidx(i) + asteroidspeed, asteroidy(i) - 10), asteroid2%
								asteroidx(i) = 5
								asteroidanimation(i) = 1
								PUT (asteroidx(i), asteroidy(i)), asteroid1%
							ELSEIF asteroidanimation(i) = 1 AND asteroidactive(i) = 1 THEN
								PUT (asteroidx(i) + asteroidspeed, asteroidy(i) - 10), asteroid1%
								asteroidx(i) = 5
								asteroidanimation(i) = 1
								PUT (asteroidx(i), asteroidy(i)), asteroid1%
						ENDIF
					ELSEIF asteroidanimation(i) = 4 AND asteroidactive(i) = 1 THEN
						PUT (asteroidx(i) + asteroidspeed,asteroidy(i)), asteroid4%
						asteroidanimation(i) = 3
						PUT (asteroidx(i),asteroidy(i)), asteroid3%
					ELSEIF asteroidanimation(i) = 3 AND asteroidactive(i) = 1 THEN
						PUT (asteroidx(i) + asteroidspeed,asteroidy(i)), asteroid3%
						asteroidanimation(i) = 2
						PUT (asteroidx(i),asteroidy(i)), asteroid2%
					ELSEIF asteroidanimation(i) = 2 AND asteroidactive(i) = 1 THEN
						PUT (asteroidx(i) + asteroidspeed,asteroidy(i)), asteroid2%
						asteroidanimation(i) = 1
						PUT (asteroidx(i),asteroidy(i)), asteroid1%
					ELSEIF asteroidanimation(i) = 1 AND asteroidactive(i) = 1 THEN
						PUT (asteroidx(i) + asteroidspeed,asteroidy(i)), asteroid1%
						asteroidanimation(i) = 4
						PUT (asteroidx(i),asteroidy(i)), asteroid4%
				ENDIF
		ENDIF
	NEXT i

	FOR i = 1 TO playerprojectilenumber 'the part that keeps track of fired projectiles yo
		IF playerprojectileactive(i) = 1 AND playerprojectiledirection(i) = 1 AND playerprojectileanimation(i) = 0 THEN 'shooting the left turret projectile which is direction 1
				PUT (playerprojectilex(i),playerprojectiley(i)), playerprojectile1% 'erases the original image with a put
				playerprojectilex(i) = playerprojectilex(i) + playerprojectilediagspeed 'diagnal speed value is less than the regular speed value
				playerprojectiley(i) = playerprojectiley(i) - playerprojectilediagspeed
				playerprojectileanimation(i) = 1 'sets the new animation
					IF playerprojectilex(i) > 315 OR playerprojectiley(i) < 5 THEN
							playerprojectileactive(i) = 0 'turns the projectile off
						ELSE
							PUT (playerprojectilex(i),playerprojectiley(i)), playerprojectile2% 'new animation swap
					ENDIF
			ELSEIF playerprojectileactive(i) = 1 AND playerprojectiledirection(i) = 1 AND playerprojectileanimation(i) = 1 THEN 'other animation for shooting to the right from the left turret
				PUT (playerprojectilex(i),playerprojectiley(i)), playerprojectile2% 'erases the original image with a put
				playerprojectilex(i) = playerprojectilex(i) + playerprojectilediagspeed 'diagnal speed value is less than the regular speed value
				playerprojectiley(i) = playerprojectiley(i) - playerprojectilediagspeed
				playerprojectileanimation(i) = 0 'sets the new animation
					IF playerprojectilex(i) > 315 OR playerprojectiley(i) < 5 THEN
							playerprojectileactive(i) = 0 'turns the projectile off
						ELSE
							PUT (playerprojectilex(i),playerprojectiley(i)), playerprojectile1% 'new animation swap
					ENDIF
			ELSEIF playerprojectileactive(i) = 1 AND playerprojectiledirection(i) = 2 AND playerprojectileanimation(i) = 0 THEN 'shooting from the center horizontally
				PUT (playerprojectilex(i),playerprojectiley(i)), playerprojectile1% 'erases the original image with a put
				playerprojectiley(i) = playerprojectiley(i) - playerprojectilespeed 'regular speed, faster than diagonal
				playerprojectileanimation(i) = 1 'sets the new animation
					IF playerprojectiley(i) < 5 THEN
							playerprojectileactive(i) = 0 'turns the projectile off
						ELSE
							PUT (playerprojectilex(i),playerprojectiley(i)), playerprojectile2% 'new animation swap
					ENDIF
			ELSEIF playerprojectileactive(i) = 1 AND playerprojectiledirection(i) = 2 AND playerprojectileanimation(i) = 1 THEN 'shooting from the center horizontally, animation swap
				PUT (playerprojectilex(i),playerprojectiley(i)), playerprojectile2% 'erases the original image with a put
				playerprojectiley(i) = playerprojectiley(i) - playerprojectilespeed 'regular speed, faster than diagonal
				playerprojectileanimation(i) = 0 'sets the new animation
					IF playerprojectiley(i) < 5 THEN
							playerprojectileactive(i) = 0 'turns the projectile off
						ELSE
							PUT (playerprojectilex(i),playerprojectiley(i)), playerprojectile1% 'new animation swap
					ENDIF
			ELSEIF playerprojectileactive(i) = 1 AND playerprojectiledirection(i) = 3 AND playerprojectileanimation(i) = 0 THEN 'shooting from the right turret, headin left
				PUT (playerprojectilex(i),playerprojectiley(i)), playerprojectile1% 'erases the original image with a put
				playerprojectilex(i) = playerprojectilex(i) - playerprojectilediagspeed 'diagnal speed value is less than the regular speed value, minus because it is going right
				playerprojectiley(i) = playerprojectiley(i) - playerprojectilediagspeed
				playerprojectileanimation(i) = 1 'sets the new animation
					IF playerprojectilex(i) < 5 OR playerprojectiley(i) < 5 THEN
							playerprojectileactive(i) = 0 'turns the projectile off
						ELSE
							PUT (playerprojectilex(i),playerprojectiley(i)), playerprojectile2% 'new animation swap
					ENDIF
			ELSEIF playerprojectileactive(i) = 1 AND playerprojectiledirection(i) = 3 AND playerprojectileanimation(i) = 1 THEN 'shooting from the right turret, headin left
				PUT (playerprojectilex(i),playerprojectiley(i)), playerprojectile2% 'erases the original image with a put
				playerprojectilex(i) = playerprojectilex(i) - playerprojectilediagspeed 'diagnal speed value is less than the regular speed value, minus because it is going right
				playerprojectiley(i) = playerprojectiley(i) - playerprojectilediagspeed
				playerprojectileanimation(i) = 0 'sets the new animation
					IF playerprojectilex(i) < 5 OR playerprojectiley(i) < 5 THEN
							playerprojectileactive(i) = 0 'turns the projectile off
						ELSE
							PUT (playerprojectilex(i),playerprojectiley(i)), playerprojectile1% 'new animation swap
					ENDIF
		ENDIF
	NEXT i
	
	FOR i = 1 TO maxexplosions 'checks every loop to see if an explosion is active for animation swapping. explosions are set active by projectile collision below
		IF explosionactive(i) = 1 AND explosionanimation(i) = 1 THEN
				PUT (explosionx(i),explosiony(i)), spacejunkexplosion1%
				explosionanimation(i) = 2
				PUT (explosionx(i),explosiony(i)), spacejunkexplosion2%
			ELSEIF explosionactive(i) = 1 AND explosionanimation(i) = 2 THEN
				PUT (explosionx(i),explosiony(i)), spacejunkexplosion2%
				explosionactive(i) = 0
		ENDIF
	NEXT i
	
	FOR i = 1 TO playerprojectilenumber
		IF playerprojectileactive(i) = 1 THEN
			FOR j = 1 TO maxasteroids 'checks to see if the center of the projectile is within the dimensions of the "box" which is the asteroid
				IF asteroidactive(j) = 1 AND (playerprojectilex(i) + 1) >= asteroidx(j) AND (playerprojectilex(i) + 1) <= (asteroidx(j) + 8) AND (playerprojectiley(i) + 1) >= asteroidy(j) AND (playerprojectiley(i) + 1) <= (asteroidy(j) + 6) THEN 
					playerprojectileactive(i) = 0
					asteroidactive(j) = 0
						IF playerprojectileanimation(i) = 0 THEN 'deletes the player projectile and the asteroid based on animation state
								PUT (playerprojectilex(i),playerprojectiley(i)), playerprojectile1%
							ELSEIF playerprojectileanimation(i) = 1 THEN
								PUT (playerprojectilex(i),playerprojectiley(i)), playerprojectile2%
						ENDIF
						IF asteroidanimation(j) = 1 THEN
								PUT (asteroidx(j),asteroidy(j)), asteroid1%
							ELSEIF asteroidanimation(j) = 2 THEN
								PUT (asteroidx(j),asteroidy(j)), asteroid2%
							ELSEIF asteroidanimation(j) = 3 THEN
								PUT (asteroidx(j),asteroidy(j)), asteroid3%
							ELSEIF asteroidanimation(j) = 4 THEN
								PUT (asteroidx(j),asteroidy(j)), asteroid4%
						ENDIF
					PUT (asteroidx(j),asteroidy(j)), spacejunkexplosion1%
						FOR k = 1 TO maxexplosions 'sets the x and y value for the explosion that will be placed so that in a different loop we can animation swap it
							IF explosionactive(k) = 0 THEN
								explosionactive(k) = 1
								explosionanimation(k) = 1
								explosionx(k) = asteroidx(j)
								explosiony(k) = asteroidy(j)
								EXIT FOR
							ENDIF
						NEXT k
				ENDIF
			NEXT j
		END IF
	NEXT i
	
	uin$ = INKEY$
		SELECT CASE uin$
			CASE "1" '1 for firing the left turret
				FOR i = 1 TO playerprojectilenumber 
					IF playerprojectileactive(i) = 0 THEN
						playerprojectileactive(i) = 1
						playerprojectileanimation(i) = 0
						playerprojectiledirection(i) = 1
						playerprojectilex(i) = leftturretx + 15
						playerprojectiley(i) = leftturrety - 3
						leftturretanimation = 1
						PUT (leftturretx,leftturrety), leftturret%
						PUT (leftturretx,leftturrety - 1), leftturretfire% 'animation swaps to the firing turret image, position offset for the firing animation
						PUT (playerprojectilex(i),playerprojectiley(i)), playerprojectile1%
						EXIT FOR
					ENDIF
				NEXT i
			CASE "2" '2 for firing the central turret
				FOR i = 1 TO playerprojectilenumber 
					IF playerprojectileactive(i) = 0 THEN
						playerprojectileactive(i) = 1
						playerprojectileanimation(i) = 0
						playerprojectiledirection(i) = 2
						playerprojectilex(i) = centralturretx + 8
						playerprojectiley(i) = centralturrety - 2
						centralturretanimation = 1
						PUT (centralturretx,centralturrety), centralturret%
						PUT (centralturretx,centralturrety - 2), centralturretfire% 'animation swaps to the firing turret image
						PUT (playerprojectilex(i),playerprojectiley(i)), playerprojectile1%
						EXIT FOR
					ENDIF
				NEXT i
			CASE "3" '2 for firing the right turret (bullet travels to the left)
				FOR i = 1 TO playerprojectilenumber 
					IF playerprojectileactive(i) = 0 THEN
						playerprojectileactive(i) = 1
						playerprojectileanimation(i) = 0
						playerprojectiledirection(i) = 3
						playerprojectilex(i) = rightturretx - 2
						playerprojectiley(i) = rightturrety - 3
						rightturretanimation = 1
						PUT (rightturretx,rightturrety), rightturret%
						PUT (rightturretx - 1,rightturrety - 1), rightturretfire% 'animation swaps to the firing turret image
						PUT (playerprojectilex(i),playerprojectiley(i)), playerprojectile1%
						EXIT FOR
					ENDIF
				NEXT i
			
			CASE CHR$(0) + CHR$(83): GOTO 1 'goes to 1 and ends the game
	
	END SELECT
				
	FOR I = 1 to 50: NEXT I 'timer for gameplay loop. Smaller number = faster game	

LOOP

1 PRINT "Game over"

END



