GET (17,94)-(19,96), playerprojectile2%
GET (21,90)-(33,101), leftturret%
GET (21,104)-(34,117), leftturretfire%
GET (35,90)-(47,101), rightturret%
GET (35,104)-(48,117), rightturretfire%

SLEEP

CLS

PUT (leftturretx,leftturrety), leftturret% 'puts the turrets at their positions. modify positions up above
PUT (centralturretx,centralturrety), centralturret%
PUT (rightturretx,rightturrety), rightturret% 

DO

_LIMIT 10

	IF leftturretanimation = 1 THEN 'the three IFs below swap back to the turret idle animation, checks every frame
		PUT (leftturretx,leftturrety - 1), leftturretfire% 'position offset for the firing animation
		PUT (leftturretx,leftturrety), leftturret%
		leftturretanimation = 0
	ENDIF
	
	IF centralturretanimation = 1 THEN
		PUT (centralturretx,centralturrety - 2), centralturretfire%
		PUT (centralturretx,centralturrety), centralturret%
		centralturretanimation = 0
	ENDIF
	
	IF rightturretanimation = 1 THEN
		PUT (rightturretx - 1,rightturrety - 1), rightturretfire%
		PUT (rightturretx,rightturrety), rightturret%
		rightturretanimation = 0
	ENDIF
	
	asteroidspawntimer = asteroidspawntimer + 5 'the timer for when an asteroid spawns. max can be set below
	cometspawntimer = cometspawntimer + 5 'same as the asteroid spawn timer, can be set in its block below
	bigasteroidspawntimer = bigasteroidspawntimer + 5
	
	IF asteroidspawntimer = 100 THEN 'set the timer here. bigger number = longer time between spawns. 
		asteroidspawntimer = 0
		asteroidspawnside = INT(RND * 2) + 1 'randomizes what side the asteroid spawn on. Either 1 or 2. 1 is left, 2 is right
		FOR i = 1 TO maxasteroids
			IF asteroidactive(i) = 0 THEN
				asteroidactive(i) = 1
					IF asteroidspawnside = 1 THEN 'from the left
							asteroiddirection(i) = 1
							asteroidanimation(i) = 1
							asteroidx(i) = 5
							asteroidy(i) = 5
							PUT (5,5), asteroid1%
						ELSEIF asteroidspawnside = 2 THEN ' from the right
							asteroiddirection(i) = 2
							asteroidanimation(i) = 4
							asteroidx(i) = 310
							asteroidy(i) = 5
							PUT (310,5), asteroid4%
					ENDIF
				EXIT FOR
			ENDIF
		NEXT i
	ENDIF
	
	FOR i = 1 TO maxasteroids
		IF asteroidactive(i) = 1 AND asteroiddirection(i) = 1 THEN
				asteroidx(i) = asteroidx(i) + asteroidspeed
				IF asteroidx(i) > 310 THEN
						asteroiddirection(i) = 2
						asteroidy(i) = asteroidy(i) + 10
						IF asteroidy(i) > 170 THEN
								asteroidactive(i) = 0
							ELSEIF asteroidanimation(i) = 4 AND asteroidactive(i) = 1 THEN 'deletes the asteroids when they "go off screen"
								PUT (asteroidx(i) - asteroidspeed, asteroidy(i) - 10), asteroid4%
								asteroidx(i) = 310
								asteroidanimation(i) = 4
								PUT (asteroidx(i), asteroidy(i)), asteroid4%
							ELSEIF asteroidanimation(i) = 3 AND asteroidactive(i) = 1 THEN
								PUT (asteroidx(i) - asteroidspeed, asteroidy(i) - 10), asteroid3%
								asteroidx(i) = 310
								asteroidanimation(i) = 4
								PUT (asteroidx(i), asteroidy(i)), asteroid4%
							ELSEIF asteroidanimation(i) = 2 AND asteroidactive(i) = 1 THEN
								PUT (asteroidx(i) - asteroidspeed, asteroidy(i) - 10), asteroid2%
								asteroidx(i) = 310
								asteroidanimation(i) = 4
								PUT (asteroidx(i), asteroidy(i)), asteroid4%
							ELSEIF asteroidanimation(i) = 1 AND asteroidactive(i) = 1 THEN
								PUT (asteroidx(i) - asteroidspeed, asteroidy(i) - 10), asteroid1%
								asteroidx(i) = 310
								asteroidanimation(i) = 4
								PUT (asteroidx(i), asteroidy(i)), asteroid4%
						ENDIF
					ELSEIF asteroidanimation(i) = 1 AND asteroidactive(i) = 1 THEN
						PUT (asteroidx(i) - asteroidspeed,asteroidy(i)), asteroid1% 'deletes the old asteroid image. remember, it was at the old area, so we minus the asteroid speed
						asteroidanimation(i) = 2
						PUT (asteroidx(i),asteroidy(i)), asteroid2%
					ELSEIF asteroidanimation(i) = 2 AND asteroidactive(i) = 1 THEN
						PUT (asteroidx(i) - asteroidspeed,asteroidy(i)), asteroid2%
						asteroidanimation(i) = 3
						PUT (asteroidx(i),asteroidy(i)), asteroid3%
					ELSEIF asteroidanimation(i) = 3 AND asteroidactive(i) = 1 THEN
						PUT (asteroidx(i) - asteroidspeed,asteroidy(i)), asteroid3%
						asteroidanimation(i) = 4
						PUT (asteroidx(i),asteroidy(i)), asteroid4%
					ELSEIF asteroidanimation(i) = 4 AND asteroidactive(i) = 1 THEN
						PUT (asteroidx(i) - asteroidspeed,asteroidy(i)), asteroid4%
						asteroidanimation(i) = 1
						PUT (asteroidx(i),asteroidy(i)), asteroid1%
				ENDIF
			ELSEIF asteroidactive(i) = 1 AND asteroiddirection(i) = 2 THEN
				asteroidx(i) = asteroidx(i) - asteroidspeed
				IF asteroidx(i) < 10 THEN
						asteroiddirection(i) = 1
						asteroidy(i) = asteroidy(i) + 10
						IF asteroidy(i) > 170 THEN
								asteroidactive(i) = 0
							ELSEIF asteroidanimation(i) = 4 AND asteroidactive(i) = 1 THEN
								PUT (asteroidx(i) + asteroidspeed, asteroidy(i) - 10), asteroid4%
								asteroidx(i) = 5
								asteroidanimation(i) = 1
								PUT (asteroidx(i), asteroidy(i)), asteroid1%
							ELSEIF asteroidanimation(i) = 3 AND asteroidactive(i) = 1 THEN
								PUT (asteroidx(i) + asteroidspeed, asteroidy(i) - 10), asteroid3%
								asteroidx(i) = 5
								asteroidanimation(i) = 1
								PUT (asteroidx(i), asteroidy(i)), asteroid1%
							ELSEIF asteroidanimation(i) = 2 AND asteroidactive(i) = 1 THEN
								PUT (asteroidx(i) + asteroidspeed, asteroidy(i) - 10), asteroid2%
								asteroidx(i) = 5
								asteroidanimation(i) = 1
								PUT (asteroidx(i), asteroidy(i)), asteroid1%
							ELSEIF asteroidanimation(i) = 1 AND asteroidactive(i) = 1 THEN
								PUT (asteroidx(i) + asteroidspeed, asteroidy(i) - 10), asteroid1%
								asteroidx(i) = 5
								asteroidanimation(i) = 1
								PUT (asteroidx(i), asteroidy(i)), asteroid1%
						ENDIF
					ELSEIF asteroidanimation(i) = 4 AND asteroidactive(i) = 1 THEN
						PUT (asteroidx(i) + asteroidspeed,asteroidy(i)), asteroid4%
						asteroidanimation(i) = 3
						PUT (asteroidx(i),asteroidy(i)), asteroid3%
					ELSEIF asteroidanimation(i) = 3 AND asteroidactive(i) = 1 THEN
						PUT (asteroidx(i) + asteroidspeed,asteroidy(i)), asteroid3%
						asteroidanimation(i) = 2
						PUT (asteroidx(i),asteroidy(i)), asteroid2%
					ELSEIF asteroidanimation(i) = 2 AND asteroidactive(i) = 1 THEN
						PUT (asteroidx(i) + asteroidspeed,asteroidy(i)), asteroid2%
						asteroidanimation(i) = 1
						PUT (asteroidx(i),asteroidy(i)), asteroid1%
					ELSEIF asteroidanimation(i) = 1 AND asteroidactive(i) = 1 THEN
						PUT (asteroidx(i) + asteroidspeed,asteroidy(i)), asteroid1%
						asteroidanimation(i) = 4
						PUT (asteroidx(i),asteroidy(i)), asteroid4%
				ENDIF
		ENDIF
	NEXT i
	
	IF cometspawntimer = 300 THEN 'set the timer here. bigger number = longer time between spawns. 
		cometspawntimer = 0
		cometspawnside = INT(RND * 2) + 1 'randomizes what side the comet spawns on. Either 1 or 2. 1 is left, 2 is right
		FOR i = 1 TO maxcomets
			IF cometactive(i) = 0 THEN
				cometactive(i) = 1
					IF cometspawnside = 1 THEN 'from the left
							cometdirection(i) = 1
							cometanimation(i) = 1
							cometx(i) = 5
							comety(i) = 15
							PUT (5,15), cometleft1%
						ELSEIF cometspawnside = 2 THEN ' from the right
							cometdirection(i) = 2
							cometanimation(i) = 3
							cometx(i) = 299
							comety(i) = 15
							PUT (299,15), cometright1%
					ENDIF
				EXIT FOR
			ENDIF
		NEXT i
	ENDIF
	
	FOR i = 1 TO maxcomets
		IF cometactive(i) = 1 AND cometdirection(i) = 1 THEN 'comets moving from left to right
				cometx(i) = cometx(i) + cometspeed
				IF cometx(i) > 300 THEN
						cometdirection(i) = 2
						comety(i) = comety(i) + 30
						IF comety(i) > 170 THEN
								cometactive(i) = 0
							ELSEIF cometanimation(i) = 1 AND cometactive(i) = 1 THEN
								PUT (cometx(i) - cometspeed,comety(i) - 30), cometleft1%
								cometx(i) = 299
								cometanimation(i) = 3
								PUT (cometx(i),comety(i)),cometright1%
							ELSEIF cometanimation(i) = 2 AND cometactive(i) = 1 THEN
								PUT (cometx(i) - cometspeed,comety(i) - 30), cometleft2%
								cometx(i) = 299
								cometanimation(i) = 3
								PUT (cometx(i),comety(i)),cometright1%
						ENDIF				
					ELSEIF cometanimation(i) = 1 THEN
						PUT (cometx(i) - cometspeed,comety(i)), cometleft1%
						cometanimation(i) = 2
						PUT (cometx(i),comety(i)), cometleft2%
					ELSEIF cometanimation(i) = 2 THEN
						PUT (cometx(i) - cometspeed,comety(i)), cometleft2%
						cometanimation(i) = 1
						PUT (cometx(i),comety(i)), cometleft1%
				ENDIF
			ELSEIF cometactive(i) = 1 AND cometdirection(i) = 2 THEN
				cometx(i) = cometx(i) - cometspeed
				IF cometx(i) < 9 THEN
						cometdirection(i) = 1
						comety(i) = comety(i) + 30 
						IF comety(i) > 170 THEN
								cometactive(i) = 0
							ELSEIF cometanimation(i) = 3 AND cometactive(i) = 1 THEN
								PUT (cometx(i) + cometspeed,comety(i) - 30), cometright1%
								cometx(i) = 5
								cometanimation(i) = 1
								PUT (cometx(i),comety(i)), cometleft1%
							ELSEIF cometanimation(i) = 4 AND cometactive(i) = 1 THEN
								PUT (cometx(i) + cometspeed,comety(i) - 30), cometright2%
								cometx(i) = 5
								cometanimation(i) = 1
								PUT (cometx(i),comety(i)), cometleft1%
						ENDIF				
					ELSEIF cometanimation(i) = 3 THEN
						PUT (cometx(i) + cometspeed,comety(i)), cometright1%
						cometanimation(i) = 4
						PUT (cometx(i),comety(i)), cometright2%
					ELSEIF cometanimation(i) = 4 THEN
						PUT (cometx(i) + cometspeed,comety(i)), cometright2%
						cometanimation(i) = 3
						PUT (cometx(i),comety(i)), cometright1%
				ENDIF
		ENDIF
	NEXT i
	
	IF bigasteroidspawntimer = 400 THEN 'set the timer here. bigger number = longer time between spawns. 
		bigasteroidspawntimer = 0
		bigasteroidspawnside = INT(RND * 2) + 1 'randomizes what side the big asteroid spawns on. Either 1 or 2. 1 is left, 2 is right
		FOR i = 1 TO maxbigasteroids
			IF bigasteroidactive(i) = 0 THEN
				bigasteroidactive(i) = 1
				bigasteroiddamaged(i) = 0
					IF bigasteroidspawnside = 1 THEN 'from the left
							bigasteroiddirection(i) = 1 '1 = moving from left to right 
							bigasteroidanimation(i) = 1
							bigasteroidx(i) = 3
							bigasteroidy(i) = 30
							PUT (3,30), bigasteroid1%
						ELSEIF bigasteroidspawnside = 2 THEN ' from the right
							bigasteroiddirection(i) = 2
							bigasteroidanimation(i) = 4
							bigasteroidx(i) = 298
							bigasteroidy(i) = 30
							PUT (298,30), bigasteroid4%
					ENDIF
				EXIT FOR
			ENDIF
		NEXT i
	ENDIF
	
	FOR i = 1 TO maxbigasteroids
		IF bigasteroidactive(i) = 1 AND bigasteroiddirection(i) = 1 THEN
				bigasteroidx(i) = bigasteroidx(i) + bigasteroidspeed
				IF bigasteroidx(i) > 298 THEN
					bigasteroiddirection(i) = 2
					bigasteroidy(i) = bigasteroidy(i) + 20
					bigasteroiddropped(i) = 1
						IF bigasteroidy(i) > 170 THEN
								bigasteroidactive(i) = 0
							ELSEIF bigasteroidanimation(i) = 4 AND bigasteroiddamaged(i) = 0 AND bigasteroidactive(i) = 1 THEN 'deletes the asteroids when they "go off screen"
								PUT (bigasteroidx(i) - bigasteroidspeed, bigasteroidy(i) - 20), bigasteroid4% 'remember, we need to check for the big asteroid damaged state too, so this will double the length of the code block...
								bigasteroidx(i) = 299
								bigasteroidanimation(i) = 4
								PUT (bigasteroidx(i), bigasteroidy(i)), bigasteroid4%
							ELSEIF bigasteroidanimation(i) = 3 AND bigasteroiddamaged(i) = 0 AND bigasteroidactive(i) = 1 THEN
								PUT (bigasteroidx(i) - bigasteroidspeed, bigasteroidy(i) - 20), bigasteroid3%
								bigasteroidx(i) = 299
								asteroidanimation(i) = 4
								PUT (bigasteroidx(i), bigasteroidy(i)), bigasteroid4%
							ELSEIF bigasteroidanimation(i) = 2 AND bigasteroiddamaged(i) = 0 AND bigasteroidactive(i) = 1 THEN
								PUT (bigasteroidx(i) - bigasteroidspeed, bigasteroidy(i) - 20), bigasteroid2%
								bigasteroidx(i) = 299
