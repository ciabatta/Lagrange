DIM rightturret%(100)
DIM rightturretfire%(100)

RANDOMIZE TIMER

DIM asteroidspawnside, cometspawnside, asteroidspeed, cometspeed, bigasteroidspawntimer, maxasteroids, maxcomets, cometspawntimer, maxbigasteroids, bigasteroidspeed, bigasteroidspawnside AS INTEGER

